{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "australiaeast"
    },
    "namePrefix": {
      "value": "acme-invoice"
    },
    "environmentName": {
      "value": "prod"
    },
    "graphTenantId": {
      "value": "REPLACE_WITH_TENANT_ID"
    },
    "graphClientId": {
      "value": "REPLACE_WITH_GRAPH_APP_CLIENT_ID"
    },
    "graphClientSecretName": {
      "value": "GRAPH-CLIENT-SECRET"
    },
    "graphScope": {
      "value": "https://graph.microsoft.com/.default"
    },
    "sharePointSiteId": {
      "value": "REPLACE_WITH_SHAREPOINT_SITE_ID"
    },
    "sharePointListId": {
      "value": "REPLACE_WITH_SHAREPOINT_LIST_ID"
    },
    "enableKeyVaultRoleAssignment": {
      "value": false
    },
    "tags": {
      "value": {
        "client": "acme",
        "owner": "cloud-consultancy",
        "costCenter": "invoice-platform",
        "application": "invoice",
        "criticality": "production"
      }
    }
  }
